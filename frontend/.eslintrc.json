{
    "plugins": ["@typescript-eslint"],
    "extends": [
        "prettier",
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "next/core-web-vitals",
        "next"
    ],
    "rules": {
        "no-console": "error",
        "@typescript-eslint/no-unused-vars": "error",
        "react/react-in-jsx-scope": "off",
        "@next/next/no-img-element": "off"
    }
}
